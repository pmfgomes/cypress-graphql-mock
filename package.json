{
  "name": "cypress-graphql-mock",
  "description": "Mock out a GraphQL schema from the client",
  "version": "0.3.1",
  "scripts": {
    "dev": "tsc -w",
    "cypress:open": "yarn cypress open --project ./test",
    "cypress:run": "yarn cypress run --project ./test/cypress",
    "test-app:run": "cd ./test/test-graphql-app && SKIP_PREFLIGHT_CHECK=true yarn start",
    "test-app:build": "cd ./test/test-graphql-app && SKIP_PREFLIGHT_CHECK=true yarn build",
    "test-app:serve": "serve -s -l 3000 test/test-graphql-app/build",
    "prepublish": "tsc"
  },
  "main": "dist",
  "types": "src",
  "files": [
    "dist/",
    "src/",
    "yarn.lock",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "Tim Griesser"
  },
  "peerDependencies": {
    "graphql": ">= 15.x.x",
    "graphql-tools": ">= 7.x.x",
    "cypress": ">= 5.x.x"
  },
  "devDependencies": {
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.1",
    "tslib": "^2.0.3",
    "@cypress/webpack-preprocessor": "^5.4.11",
    "@types/graphql": "^14.5.0",
    "cypress": "^5.6.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "wait-on": "^5.2.0",
    "webpack": "^5.4.0"
  }
}
